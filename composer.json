{
    "name": "icicleio/websocket",
    "description": "Asynchronous WebSocket component for Icicle.",
    "keywords": [
        "websocket",
        "asynchronous",
        "async"
    ],
    "homepage": "http://icicle.io",
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@icicle.io"
        }
    ],
    "require": {
        "icicleio/icicle": "^2.0",
        "icicleio/socket": "^2.0",
        "icicleio/http": "^2.0",
        "icicleio/log": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5",
        "symfony/yaml": "^2.7"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Icicle\\WebSocket\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Icicle\\Examples\\WebSocket\\": "examples",
            "Icicle\\Tests\\WebSocket\\": "tests"
        }
    }
}
